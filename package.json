{
	"name": "s7s",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "npm run preflight && vite dev",
		"build": "npm run preflight && npm run check && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"preflight": "node ./_env.mjs",
		"postinstall": "prisma generate"
	},
	"devDependencies": {
		"@clerk/clerk-js": "^4.38.4",
		"@clerk/clerk-sdk-node": "^4.8.6",
		"@prisma/client": "^4.13.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^2.4.1",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/forms": "^0.5.3",
		"@trpc/client": "^10.21.1",
		"@trpc/server": "^10.21.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/node": "^18.15.11",
		"@types/qs": "^6.9.7",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@upstash/ratelimit": "^0.4.2",
		"@vercel/analytics": "^1.0.0",
		"autoprefixer": "^10.4.14",
		"cookies": "^0.8.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsonwebtoken": "^9.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.7",
		"prisma": "^4.13.0",
		"qs": "^6.11.1",
		"superjson": "^1.12.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.1",
		"trpc-sveltekit": "^3.5.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
